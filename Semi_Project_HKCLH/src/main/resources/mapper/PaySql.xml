<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="data.mapper.PayMapperInter">
	<insert id="insertPay" parameterType="Pay">
		insert into pay (mid,pydate,pyname,pyprice,pyphoto,pyoption,pyquantity,pydelivery)
		 values (#{mid},now(),#{pyname},#{pyprice},#{pyphoto},#{pyoption},#{pyquantity},"배송 준비중")
	</insert>
</mapper>